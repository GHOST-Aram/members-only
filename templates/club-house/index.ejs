<%-include('../partials/head.ejs') %>
<body>
    <%-include('../partials/nav.ejs') %>
    <h1>Welcome to Club house</h1>
    <% if(posts.length){ %>
        <ul>
            <h1>Posts</h1>
            <% posts.map(post =>{ %>
                <li>
                    <a href="<%=post.url%>">
                        <h2><%=post.title%></h2>
                    </a>
                </li>
          <%})%>
        </ul>
    <%}%>
    <%-include('../partials/footer.ejs') %>

</body>
</html>